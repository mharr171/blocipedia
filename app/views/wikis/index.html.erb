<h1>Wikis</h1>

<div class='row'>
  <div class='col-md-8'>
    <% @wikis.each do |wiki| %>
      <%= render wiki %>
    <% end %>
  </div>
  <div class='col-md-4'>
    <% if current_user %>
      <%= link_to 'Create Wiki', new_wiki_path, :class=>'btn btn-primary btn-sm' %>
    <% end %>
  </div>
</div>
